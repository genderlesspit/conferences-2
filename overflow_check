for_each_loop = {
	array = var_overflow_check
	value = v
	set_temp_variable = { next_decimal = v }
	add_to_temp_variable = { next_decimal = 1 }
	set_temp_variable = { this_decimal = v }
	set_temp_variable = { last_decimal = v }
	add_to_temp_variable = { last_decimal = -1 }
	if = { 
		limit = { check_var = { var_overflow_check^this_decimal > 9 } } #is value 10?
		add_to_variable = { var_overflow_check^next_decimal = 1 } #carries over
		add_to_variable = { var_overflow_check^this_decimal = -10 } #result is 0
	}
	if = { 
		limit = { check_var = { var_overflow_check^this_decimal < 0 } } #is value -1
		add_to_variable = { var_overflow_check^this_decimal = -1 } #carries over
		add_to_variable = { var_overflow_check^last_decimal= 9 } #result is 0
	}
}
